<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm orm_2_0.xsd"
  version="2.0">
  <entity class="poo.library.modelo.Biblioteca" access="FIELD" metadata-complete="true">
    <table name="biblioteca" />
    <attributes>
      <id name="id">
        <column name="id" />
        <generated-value strategy="IDENTITY" />
      </id>

      <basic name="nome" />
      <basic name="multaDiaria" />

      <basic name="qteDiasLocacao" />
      <basic name="qteDiasValidadeReserva" />

      <one-to-many name="acervo"></one-to-many>

      <transient name="alugueis" />
      <transient name="reservas" />
      <transient name="usuarios" />
    </attributes>
  </entity>
</entity-mappings>